{
  "extends": [
    "github>nuxt/renovate-config-nuxt"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "ignoreDeps": [
    "happy-dom"
  ],
  "baseBranches": ["dev", "v3"],
  "packageRules": [{
    "matchBaseBranches": ["v3"],
    "labels": ["v3"]
  }, {
    "matchBaseBranches": ["dev"],
    "matchPackageNames": [
      "@nuxt/eslint-config",
      "eslint",
      "valibot30",
      "valibot31"
    ],
    "enabled": false
  }]
}
